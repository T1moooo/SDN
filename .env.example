# Example .env file
# Copy this to .env and fill in your actual switch details

# IP address or hostname of the Nexus switch
SWITCH_IP=192.168.1.1

# Credentials for NX-API
SWITCH_USERNAME=admin
SWITCH_PASSWORD=your_secure_password

# Set to "false" if your switch's NX-API is running on HTTP instead of HTTPS
NXAPI_USE_HTTPS=True

# Flask application settings
FLASK_PORT=5001
DEBUG=True
